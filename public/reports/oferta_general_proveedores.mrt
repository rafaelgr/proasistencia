        "NameInSource": "PROAS_REPORT",
        "SqlCommand": "SELECT o.ofertaId, o.referencia, DATE_FORMAT(o.fechaOferta,'%d-%m-%Y') AS fechaOferta,\r\ne.nombre AS nombreEmpresa, e.nif AS nifEmpresa,\r\nTRIM(CONCAT(COALESCE(tv1.nombre, \"\"), \" \" , e.direccion)) AS empresaDireccion, \r\nTRIM(CONCAT(COALESCE(tv3.nombre, \"\"), \" \" , p.direccion)) AS proveedorDireccion, \r\ne.codPostal AS empresaCodPostal, e.poblacion AS empresaPoblacion, e.provincia AS empresaProvincia,\r\nc.nombre AS clienteNombre, c.nif AS clienteNif, ol.importeProveedor,\r\nTRIM(CONCAT(COALESCE(tv2.nombre, \"\"), \" \", c.direccion)) AS clienteDireccion, \r\nc.codPostal AS clienteCodPostal, c.poblacion AS clientePoblacion, c.provincia AS clienteProvincia,\r\no.total, o.totalConIva, o.observaciones, \r\nfp.nombre AS formaPago, ol.proveedorId, p.nombre as proveedornombre, p.nif AS proveedorNif, p.codPostal AS proveedorCodpostal, p.poblacion AS proveedorPoblación,\r\np.provincia AS proveedorProvincia\r\nFROM ofertas AS o\r\nLEFT JOIN empresas AS e ON e.empresaId = o.empresaId\r\nLEFT JOIN clientes AS c ON c.clienteId = o.clienteId\r\nLEFT JOIN ofertas_lineas AS ol ON ol.ofertaId = o.ofertaId\r\nLEFT JOIN proveedores AS p ON p.proveedorId = ol.proveedorId\r\nLEFT JOIN formas_pago AS fp ON fp.formaPagoId = o.formaPagoId\r\nLEFT JOIN tipos_via AS tv1 ON tv1.tipoViaId = e.tipoViaId\r\nLEFT JOIN tipos_via AS tv2 ON tv2.tipoViaId = c.tipoViaId\r\nLEFT JOIN tipos_via AS tv3 ON tv3.tipoViaId = p.tipoViaId"
      },
