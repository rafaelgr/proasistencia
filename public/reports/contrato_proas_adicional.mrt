              "Text": {
                "Value": "<font face=\"Arial\" size=\"10\">ADICIONAL 1.- Presupuesto de Ref. {datos_contrato.referencia}</font>"
              },
              "Font": ";9;;",
              "Border": ";;;;;;;solid:Black",
              "Brush": "solid:",
