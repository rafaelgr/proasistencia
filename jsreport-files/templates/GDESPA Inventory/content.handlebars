<meta charset="utf-8">
<!-- #CSS Links -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
<!--
<link rel="stylesheet" href="http://localhost:9000/ficheros/css/bootstrap.min.css">
-->
<img src='{#image logo-gdes-lineman}' style="display:none;"/>
<div class="well">
    <div style="text-align:right;">
        <h1>{{nreport}}</h1>
    </div>
    <table class="table table-stripped table-forum">
        <thead class="txt-color-blue">
            <tr>
                <th>FECHA</th>
                <th>RESPONSABLE</th>
                <th>ALMACEN</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>{{inventoryDate}}</td>
                <td>{{worker}}</td>
                <td>{{store}}</td>
            </tr>
        </tbody>
    </table> 
    <div>
        <strong>Observaciones:</strong>    
    </div>
    <div>
        {{comments}}
    </div>
    <hr/>
    <table class="table table-stripped table-forum  table-bordered">
        <thead class="txt-color-blue">
            <tr>
                <th>Articulo</th>
                <th>Cantidad</th>
            </tr>
        </thead>
        <tbody>
            {{#each lines}}
            <tr>
                <td>{{item}}</td>
                <td></td>
            </tr>
            {{/each}}
        </tbody>
    </table>            
</div>